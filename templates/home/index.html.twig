{% extends 'base.html.twig' %}

{% block title %}Homepage - {{ parent() }}{% endblock %}

{% block stylesheets %}
    <link href="https://fonts.googleapis.com/css?family=Lato:100" rel="stylesheet" type="text/css">
    {{ parent() }}
    <style>
        body {
            margin: 0;
            padding: 0;
            font-weight: 100;
            font-family: 'Lato';
        }

        .title {
            margin: 20px auto;
            text-align: center;
            font-size: 96px;
        }
    </style>
{% endblock %}

{% block body %}
    <div class="title">NFQ Akademija</div>
    <img src="{{ url }}"/>
    {{ url }}


    <hr/>
    <div>Code: {{ code }}</div>
    <pre>{{ dump(valid) }}</pre>
    <hr/>
    <form action="/">
        <input type="text" name="code">
        <input type="submit">
    </form>
{% endblock %}
